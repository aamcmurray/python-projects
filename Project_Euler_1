# Find the sum of all the multiples of 3 or 5 below 1000.

def finder(c,n):
	values=[False]*c
	i=1
	while i < c:
		if (i%n)!=0:
			i+=1
			continue
		elif (i%n) == 0:
			yield i
			values[i]=True
			j=n*i
			while j < c:
				values[j]=True
				j+=1
			i+=1
	return

def sum_lists(p,q):
	s=sum(set(p+q))
	return s

def main():
	ceiling=1000
	num_list_3=list(finder(ceiling,3))
	num_list_5=list(finder(ceiling,5))
	print(sum_lists(num_list_3,num_list_5))

if __name__ == "__main__":
	main()
