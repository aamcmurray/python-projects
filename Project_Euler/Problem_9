#  There exists exactly one Pythagorean triplet (PT) for which a + b + c = 1000. A PT is a set of natural nums a<b<c for which a^2+b^2=c^2
import numpy as np

def check_pathagorean(a, b, c):
	return (a**2) + (b**2) == (c**2)

def finder(n):
	for i in range(1, n):
		for j in range(i+1, n):
			c = summation - (i + j)
			if(check_pathagorean(i, j, c)):
				if i + j + c == summation:
					print(i*j*c)
					break
					
def main():
	summation=1000
	finder(n)

if __name__ == "__main__":
	main()
